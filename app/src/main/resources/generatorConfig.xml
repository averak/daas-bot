<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
	<context id="schema" targetRuntime="MyBatis3">
		<property name="autoDelimitKeyWords" value="true" />
		<property name="beginningDelimiter" value="`" />
		<property name="endingDelimiter" value="`" />
		<plugin type="com.softwareloop.mybatis.generator.plugins.LombokPlugin">
			<property name="builder" value="true" />
			<property name="allArgsConstructor" value="true" />
		</plugin>

		<jdbcConnection driverClass="com.mysql.jdbc.Driver" connectionURL="jdbc:mysql://localhost:3306/daas" userId="daas" password="daas" />
		<javaModelGenerator targetPackage="dev.abelab.daas.db.entity" targetProject="app/src/main/java" />
		<sqlMapGenerator targetPackage="dev.abelab.daas.db.mapper" targetProject="app/src/main/resources" />
		<javaClientGenerator targetPackage="dev.abelab.daas.db.mapper" targetProject="src/main/java" type="XMLMAPPER" />

		<table tableName="dajare">
			<generatedKey column="id" sqlStatement="JDBC" identity="true" />
		</table>

	</context>
</generatorConfiguration>
